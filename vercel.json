{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "wdb-de.de" }],
      "destination": "https://www.wdb-de.de/:1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.wdb-de.de" }],
      "destination": "https://app---wdb-1dcc6e3.base44.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=15552000; includeSubDomains; preload" },
        { "key": "Content-Security-Policy", "value": "default-src 'self' https: data: blob:;" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "same-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" }
      ]
    }
  ]
}
